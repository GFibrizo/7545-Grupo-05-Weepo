      <aside data-ng-include data-src="'app/components/core/sidebar/sidebarView.html'" 
             data-ng-controller="SidebarCtrl" 
             class="main-sidebar">
      </aside>

      <!-- Content Wrapper. Contains page content -->
      <div id="content-wrapper" 
           class="content-wrapper"
           data-ng-view="">
      </div>

      <!-- Footer --> 
      <footer data-ng-include data-src="'app/components/core/footer/footerView.html'" 
